@model ProcessAddDto
@{
    ViewData["Title"] = "CreateProcess";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Create Process</h1>
<br />
<form method="post">
    <span>Receiver</span>
    <input asp-for="ReceiverId" type="text" class="form-control" placeholder="Receiver" />
    <br />
    <span>Amount</span>
    <input asp-for="Amount" type="text" class="form-control" placeholder="Amount" />
    <br />
    <span>Type</span>
    <input asp-for="Type" type="text" class="form-control" placeholder="Type" />
    <br />
    <input type="submit" class="form-control" value="Save Changes" />
</form>
